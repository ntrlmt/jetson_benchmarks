FROM nvcr.io/nvidia/l4t-ml:r32.4.4-py3

RUN apt update && \ 
    apt install -y sudo && \ 
    apt install -y unzip && \
    apt install -y nvidia-jetpack
COPY . /jetson_benchmarks
WORKDIR /jetson_benchmarks
RUN mkdir models && sh install_requirements.sh && \
    python3 utils/download_models.py --all --csv_file_path /jetson_benchmarks/benchmark_csv/tx2-nano-benchmarks.csv --save_dir /jetson_benchmarks/models
